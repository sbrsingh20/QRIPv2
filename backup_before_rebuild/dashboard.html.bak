                            <button class="mode-btn" data-mode="tracking">üéØ Tracking</button>
                            <button class="mode-btn" data-mode="targeting">üîí Targeting</button>
                        </div>

                        <!-- IFF Legend -->
                        <div class="iff-legend">
                            <div class="legend-item">
