                        <div id="radarFleetList" class="scrolling-list"
                            style="flex-grow: 1; overflow-y: auto; border: 1px solid rgba(0,212,255,0.1); margin-top: 10px;">
                            <!-- Populated by JS -->
                            <div class="loading-text">Connecting to Fleet Command...</div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

